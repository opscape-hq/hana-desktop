{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "hana-cli",
  "version": "0.1.0",
  "identifier": "cc.mcjk.hana-cli",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/logo-32.png",
      "icons/logo-64.png",
      "icons/logo-128.png",
      "icons/logo-512.png",
      "icons/logo-1024.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
